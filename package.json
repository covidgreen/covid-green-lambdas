{
  "name": "contact-tracing-lambda",
  "version": "1.0.0",
  "private": true,
  "description": "Common lambda functions for contact tracing",
  "scripts": {
    "artifact:create": "zip -r lambdas.zip *",
    "exposures": "env-cmd -f .env node exposures.js",
    "settings": "env-cmd -f .env node settings.js",
    "stats": "env-cmd -f .env node stats.js",
    "token:metrics": "env-cmd -f .env node token.js \"{ \\\"type\\\": \\\"metrics\\\", \\\"description\\\": \\\"Manual token\\\" }\"",
    "token:push": "env-cmd -f .env node token.js \"{ \\\"type\\\": \\\"push\\\", \\\"description\\\": \\\"Manual token\\\" }\"",
    "token:register": "env-cmd -f .env node token.js \"{ \\\"type\\\": \\\"register\\\", \\\"description\\\": \\\"Manual token\\\" }\""
  },
  "dependencies": {
    "@nearform/sql": "^1.4.0",
    "archiver": "^4.0.1",
    "aws-sdk": "^2.646.0",
    "axios": "^0.19.2",
    "env-schema": "^1.1.0",
    "flat": "^5.0.0",
    "fluent-schema": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.0",
    "node-jose": "^1.1.4",
    "pg": "^7.18.2",
    "pg-range": "^1.1.0",
    "protobufjs": "^6.9.0"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "prettier": "prettier-config-standard"
}
